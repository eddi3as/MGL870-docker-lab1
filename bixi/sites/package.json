{
  "name": "lab2",
  "version": "1.0.0",
  "description": "mean app for bikes ETS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "stop": "taskkill /f /im node.exe",
    "deploy": "npm run build-dev && npm run start-dev-doc",
    "build": "npm run build-dev && npm run copyassets",
    "build-dev": "tsc -b --verbose && npm run copyviews",
    "copyviews": "copyfiles --all ./views/* ./dist",
    "copyassets": "copyfiles --all ./views/assets/* ./dist",
    "start-dev": "SET NODE_ENV=dev && node dist/index.js",
    "start-dev-doc": "node --require ./dist/instrumentation.js dist/index.js",
    "start-dev-mac": "export NODE_ENV=dev && node dist/index.js",
    "start": "SET NODE_ENV=production && node dist/index.js"
  },
  "author": "Equipe 8 GTI525",
  "license": "ISC",
  "dependencies": {
    "@opentelemetry/api": "=1.9.0",
    "@opentelemetry/auto-instrumentations-node": "=0.50.0",
    "@opentelemetry/exporter-jaeger": "^1.26.0",
    "@opentelemetry/exporter-trace-otlp-http": "=0.53.0",
    "@opentelemetry/instrumentation": "^0.53.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-metrics": "=1.26.0",
    "@opentelemetry/sdk-node": "=0.53.0",
    "@opentelemetry/sdk-trace-node": "=1.26.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@opentelemetry/tracing": "^0.24.0",
    "body-parser": "^1.20.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jaeger-client": "^3.19.0",
    "moment": "^2.29.3",
    "mongoose": "^6.3.5",
    "morgan": "^1.10.0",
    "opentracing": "^0.14.7",
    "prom-client": "^15.1.3",
    "response-time": "^2.3.2",
    "shx": "^0.3.4",
    "typescript": "^4.7.3",
    "uuid": "^8.3.2",
    "winston": "^3.15.0",
    "winston-loki": "^6.1.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3"
  }
}
